{
    "compilerOptions": {
      "module": "node16",
      "target": "es2018",
      "moduleResolution": "node16",
      "allowJs": true,
      "declaration": true,
      "declarationMap": true,
      "strict": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "newLine": "lf"
    },
    "include": [],
    "ts-node": {
        "files": true,
        "swc": true,
        "compilerOptions": {
            "module": "commonjs"
        }
    },
    "references": [
      {
        "path": "packages/fasset-indexer-core"
      },
      {
        "path": "packages/fasset-indexer-api"
      }
    ]
  }
