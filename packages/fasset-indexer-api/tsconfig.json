{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "removeComments": true,
    "baseUrl": "./",
    "incremental": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false
  },
  "include": [
    "src/**/*",
    "test/**/*"
  ],
  "references": [
    {
      "path": "../fasset-indexer-core"
    }
  ]
}